{
  "name": "expo-demo",
  "main": "expo-router/entry",
  "version": "0.0.1",
  "scripts": {
    "start": "STAGE=development expo start --dev-client",
    "expo:prebuild": "expo prebuild --skip-dependency-update react,react-native,expo",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "ios:development": "yarn clean:native && STAGE=development yarn expo:prebuild && STAGE=development yarn ios",
    "android:development": "yarn clean:native && STAGE=development yarn expo:prebuild && STAGE=development yarn android",
    "ios:preview": "STAGE=preview yarn expo:prebuild && STAGE=preview yarn ios",
    "android:preview": "STAGE=staging yarn expo:prebuild && STAGE=staging yarn android",
    "build:development": "STAGE=development eas build --profile development --platform all",
    "build:development:ios": "STAGE=development eas build --profile development --platform ios",
    "build:development:android": "STAGE=development eas build --profile development --platform android",
    "build:preview": "STAGE=preview eas build --profile preview --platform all",
    "build:preview:ios": "STAGE=preview eas build --profile preview --platform ios",
    "build:preview:android": "STAGE=preview eas build --profile preview --platform android",
    "clean:native": "rm -rf android && rm -rf ios && rm -rf .expo",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@bottom-tabs/react-navigation": "^0.9.2",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/native": "^7.1.6",
    "auto-zustand-selectors-hook": "^3.0.1",
    "expo": "~53.0.17",
    "expo-blur": "~14.1.5",
    "expo-build-properties": "~0.14.8",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "^5.2.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.3.2",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.3",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "i18n-js": "^4.5.1",
    "i18next": "^25.3.1",
    "immer": "9.0.12",
    "intl-pluralrules": "^2.0.1",
    "lodash": "^4.17.21",
    "node-libs-react-native": "^1.2.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-i18next": "^15.6.0",
    "react-native": "0.79.5",
    "react-native-bottom-tabs": "^0.9.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-mmkv": "^3.3.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "styled-components": "^6.1.19",
    "tinycolor2": "^1.6.0",
    "zod": "^3.25.74",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@types/lodash": "^4.17.20",
    "@types/react": "~19.0.10",
    "@types/styled-components": "5.1.34",
    "@types/styled-components-react-native": "5.2.5",
    "@types/tinycolor2": "^1.4.6",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "typescript": "~5.8.3"
  },
  "private": true,
  "expo": {
    "autolinking": {
      "nativeModulesDir": "../../packages"
    }
  }
}
